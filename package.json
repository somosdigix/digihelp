{
  "name": "digihelp",
  "description": "Um componente frontend que cria uma modal de intruções para o usuário.",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run node-sass && webpack --optimize-minimize",
    "build-watch": "webpack --watch --progress --optimize-minimize",
    "watch-css": "nodemon -e scss -x \"npm run node-sass\"",
    "node-sass": "node-sass --include-path sass sass/_digix-ui-base.scss css/main.css --output-style compressed",
    "server": "browser-sync start --server --files ./* --index ./index.html",
    "start": "npm-run-all -p -r watch-css build-watch server"
  },  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/somosdigix/digihelp.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/somosdigix/digihelp/issues"
  },
  "homepage": "https://github.com/somosdigix/digihelp#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.26.3",
    "css-loader": "^1.0.0",
    "digix-ui": "^2.4.25",
    "html-loader": "^0.5.5",
    "node-sass": "^4.9.4",
    "nodemon": "^1.18.5",
    "npm-run-all": "^4.1.3",
    "style-loader": "^0.22.1",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "readmeFilename": "readme.md",
  "keywords": ["helper", "ajuda", "digix", "somosdigix"],
  "tags": ["helper", "ajuda", "digix", "somosdigix"],
  "engines": {
    "node": ">=0.8.0"
  },
  "main": "dist/digihelp.js"
}
